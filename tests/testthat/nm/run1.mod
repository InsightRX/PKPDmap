;; 3. Label:

$PROBLEM PK

$INPUT ID TIME DV AMT CMT MDV EVID WT CRCL AGE SEX CENT

$DATA pktab1 IGNORE=@;

$SUBROUTINES ADVAN1 TRANS2

$PK
TVCL = THETA(1)
CL = TVCL * EXP(ETA(1))
TVV = THETA(2)
V = TVV * EXP(ETA(2))
S1 = V/1000

$ERROR
IPRED = F
ADD = SQRT(SIGMA(1,1))
W = ADD
Y = IPRED + EPS(1)
IRES = DV-IPRED
IWRES = IRES/W

$THETA
(2,  10, 100) ; CL
(20, 100, 500) ; V

$OMEGA BLOCK(2)
0.2 ; IIV CL
0.1 0.2 ; IIV V2

$SIGMA
100 ; Additive error PK

$EST METHOD=1 INTER MAXEVAL=2000 NOABORT SIG=3 PRINT=1 POSTHOC
$COV

$TABLE ID TIME DV AMT CMT MDV EVID WT CRCL AGE SEX CENT IPRED IWRES
NOPRINT ONEHEADER NOAPPEND FILE=mytab1

; Xpose
$TABLE ID TIME DV MDV EVID IPRED IWRES CWRES ONEHEADER NOPRINT FILE=sdtab1
$TABLE CL V ADD ONEHEADER NOPRINT FILE=patab1

